{
    "name": "arna-energy-website",
    "version": "1.0.0",
    "description": "ARNA Energy Company Website with Admin Panel",
    "main": "backend/server.js",
    "engines": {
      "node": "18.x"
    },
    "scripts": {
      "start": "node backend/server.js",
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "server": "cd backend && nodemon server.js",
      "client": "cd frontend && npm start",
      "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install"
    },
    "keywords": [
      "energy",
      "industrial",
      "website",
      "admin-panel"
    ],
    "author": "ARNA Energy",
    "license": "MIT",
    "dependencies": {
      "bcryptjs": "^2.4.3",
      "cloudinary": "^1.41.3",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-rate-limit": "^7.1.5",
      "helmet": "^7.1.0",
      "jsonwebtoken": "^9.0.2",
      "multer": "^1.4.4",
      "multer-storage-cloudinary": "^4.0.0",
      "mysql2": "^3.6.5"
    },
    "devDependencies": {
      "concurrently": "^8.2.2",
      "nodemon": "^3.0.2"
    }
  }
  