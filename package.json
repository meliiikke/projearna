{
  "name": "arna-energy-website",
  "version": "1.0.0",
  "description": "ARNA Energy Company Website with Admin Panel",
  "main": "server.js",

  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd frontend && npm start",
    "install-all": "npm install && cd frontend && npm install",
    "build": "npm run build:frontend",
    "build:backend": "npm install",
    "build:frontend": "cd frontend && npm run build",
    "build:check": "npm run build:backend && npm run build:frontend",
    "postinstall": "echo 'Dependencies installed successfully'"
  },
  "keywords": ["energy", "industrial", "website", "admin-panel"],
  "author": "ARNA Energy",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.4",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
